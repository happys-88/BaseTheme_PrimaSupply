<div class="mz-productlist {% block module-classes %}{% endblock module-classes %}">
    {% if model.items %}
    <ul class="mz-productlist-list {% block list-classes %}{% endblock list-classes %} clearfix">
        {% for prod in model.items %}
            <li class="mz-productlist-item col-md-3 col-sm-6 col-xs-6" data-mz-product="{{ prod.productCode }}">{% include "modules/product/product-listing" with model=prod %}</li>
        {% endfor %}
    </ul>
    {% endif %}
</div>